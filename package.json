    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/webpack-config": "^19.0.1",
    "axios": "^1.8.2",
    "axios-retry": "^4.5.0",
    "expo-dev-client": "~5.0.13",
    "expo-font": "~13.0.4",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.3",
    "nativewind": "latest",
    "react": "18.3.1",
    "react-dom": "18",
    "react-native": "0.76.7",
    "react-native-fast-image": "^8.6.3",
